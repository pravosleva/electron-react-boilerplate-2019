{
  "name": "electron-react-boilerplate-2019",
  "version": "1.0.0",
  "description": "A minimal Electron + React application",
  "main": "main.js",
  "scripts": {
    "install-all": "yarn install && yarn --cwd ./frontend install",
    "build-client": "yarn --cwd ./frontend build",
    "start": "NODE_ENV=production electron .",
    "fresh-prod-start": "yarn build-client && NODE_ENV=production yarn start",
    "frontend-dev": "yarn --cwd ./frontend dev",
    "dev": "concurrently --kill-others \"yarn frontend-dev\" \"NODE_ENV=development electron .\"",
    "dev-1": "yarn frontend-dev && NODE_ENV=development electron ."
  },
  "repository": "https://github.com/pravosleva/electron-react-boilerplate-2019",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "electron": "^5.0.6"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
